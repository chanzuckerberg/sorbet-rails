# Rails app
/spec/support/**/tmp/*
/spec/support/**/.bundle

# Ignore the default SQLite database.
/spec/support/**/db/*.sqlite3
/spec/support/**/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/spec/support/**/log/*
/spec/support/**/tmp/*
!/spec/support/**/log/.keep
!/spec/support/**/tmp/.keep

# Ignore uploaded files in development
/spec/support/**/storage/*
!/spec/support/**/storage/.keep

/spec/support/**/node_modules
/spec/support/**/yarn-error.log

/spec/support/**/public/assets
.byebug_history

# Ignore master key for decrypting credentials and the secrets.yml that was used in older versions of Rails.
/spec/support/**/config/master.key
/spec/support/**/config/secrets.yml
/spec/support/**/config/credentials.yml.enc

# Ignore sorbet generated files
/sorbet/rbi/hidden-definitions/
/spec/support/**/sorbet/*
/spec/support/**/sorbet/**/*

# Ignore files in the public/ directories.
/spec/support/**/public/*

